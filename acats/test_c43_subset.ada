WITH REPORT; USE REPORT;
PROCEDURE TEST_C43_SUBSET IS
     TYPE ARR22 IS ARRAY (IDENT_INT(-1) .. IDENT_INT(1),
                          IDENT_INT(-1) .. IDENT_INT(1)) OF INTEGER;
     VA22 : ARR22 := (-1 => (1,1,1), 0..1 => (OTHERS => IDENT_INT(2)));
BEGIN
     TEST ("SUBSET", "VA22 CHECK");
     IF VA22(-1,-1) /= 1 THEN FAILED("VA22(-1,-1)"); END IF;
     IF VA22(-1,0) /= 1 THEN FAILED("VA22(-1,0)"); END IF;
     IF VA22(-1,1) /= 1 THEN FAILED("VA22(-1,1)"); END IF;
     IF VA22(0,-1) /= 2 THEN FAILED("VA22(0,-1)"); END IF;
     IF VA22(0,0) /= 2 THEN FAILED("VA22(0,0)"); END IF;
     IF VA22(1,1) /= 2 THEN FAILED("VA22(1,1)"); END IF;
     IF VA22 /= ((1,1,1), (2,2,2), (2,2,2)) THEN
          FAILED("VA22 FULL COMPARE");
     END IF;
     RESULT;
END TEST_C43_SUBSET;
